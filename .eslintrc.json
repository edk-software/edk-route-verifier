{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:jest/recommended"
    ],
    "plugins": [
        "jest",
        "prettier"
    ],
    "rules" : {
        "import/extensions": 0,
        "prettier/prettier": ["error"]
    },
    "env" : {
        "node" : true
    }
}
